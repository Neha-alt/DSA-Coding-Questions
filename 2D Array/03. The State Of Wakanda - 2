import java.io.*;
import java.util.*;

public class Main {

    public static void main(String[] args) throws Exception {
        Scanner sc = new Scanner(System.in);
        int r = sc.nextInt();
        int a[][] = new int[r][r];
        for (int i = 0; i < r; i++) {
            for (int j = 0; j < r; j++) {
                a[i][j] = sc.nextInt();
            }
        }
        for (int g = 0; g < a.length; g++) {
            for (int i = 0, j = g; j < a.length; i++, j++) {
                System.out.println(a[i][j]);
            }
        }
    }

}
